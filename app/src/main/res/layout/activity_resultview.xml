<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#000000"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tvspo14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/robotolight"
        android:text="AGE "
        android:textColor="@color/cardview_light_background"
        android:textSize="18sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_result_pulse_rate" />

    <TextView
        android:id="@+id/tvspo16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/robotolight"
        android:text="Neonate ( 28 days) "
        android:textColor="@color/colorRecieveText"
        android:textSize="18sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvspo15" />

    <TextView
        android:id="@+id/tvspo18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/robotolight"
        android:text="Infant (1 month - 1 year) "
        android:textColor="@color/colorRecieveText"
        android:textSize="18sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvspo16" />

    <TextView
        android:id="@+id/tvspo19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/robotolight"
        android:text="Todler "
        android:textColor="@color/colorRecieveText"
        android:textSize="18sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvspo18" />

    <TextView
        android:id="@+id/tvspo20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/robotolight"
        android:text="Pre-school "
        android:textColor="@color/colorRecieveText"
        android:textSize="18sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvspo19" />

    <TextView
        android:id="@+id/tvspo21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/robotolight"
        android:text="6-11 years "
        android:textColor="@color/colorRecieveText"
        android:textSize="18sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvspo20" />

    <TextView
        android:id="@+id/tvspo22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/robotolight"
        android:text="12 years - adult "
        android:textColor="@color/colorRecieveText"
        android:textSize="18sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvspo21" />

    <TextView
        android:id="@+id/tvspo23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/robotolight"
        android:text="Athlete "
        android:textColor="@color/colorRecieveText"
        android:textSize="18sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvspo22" />

    <TextView
        android:id="@+id/tvspo15"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Normal Pulse Rate"
        android:textColor="@color/cardview_light_background"
        android:textSize="18sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tvspo17"
        app:layout_constraintTop_toTopOf="@+id/tvspo14"
        android:fontFamily="@font/robotolight" />

    <TextView
        android:id="@+id/tvspo17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/robotolight"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:text="100 - 205 "
        android:textColor="@color/colorRecieveText"
        android:textSize="18sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintStart_toStartOf="@+id/tvspo24"
        app:layout_constraintTop_toTopOf="@+id/tvspo16" />

    <TextView
        android:id="@+id/tvspo26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/robotolight"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:text="80 - 120 "
        android:textColor="@color/colorRecieveText"
        android:textSize="18sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintStart_toStartOf="@+id/tvspo25"
        app:layout_constraintTop_toTopOf="@+id/tvspo20" />

    <TextView
        android:id="@+id/tvspo27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/robotolight"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:text="75 - 118 "
        android:textColor="@color/colorRecieveText"
        android:textSize="18sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintStart_toStartOf="@+id/tvspo26"
        app:layout_constraintTop_toTopOf="@+id/tvspo21" />

